sno=int(input('enter sno'))
sname=input('enter name')
s1=int(input('s1 marks'))
s2=int(input('s2 marks'))
s3=int(input('s3 marks'))
total=s1+s2+s3
avg=total/3
if avg>=91:
    print('o-grade')
elif avg>=81:
    print('A-grade')
elif avg>=71:
    print('B-grade')
elif avg >=61:
    print('C Grade')
elif avg >=51:
    print(' D Grade')
elif avg >=41:
    print(' E Grade')
else:
    print("You are fail")
